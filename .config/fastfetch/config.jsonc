        {
            "type": "locale",
            "key": "{icon}  Locale               ",
            "keyColor": "blue"
        },
        {
            "type": "break",
            "key": "{icon}"
        },
        {
            "type": "datetime",
            "key": "{icon}  Date & Time          ",
            "keyColor": "blue"
        },
        {
            "type": "uptime",
            "key": "{icon}  Uptime               ",
            "keyColor": "blue",
            "outputColor": "bright_red"
        },
        {
            "type": "command",
            "key": "{icon}  OS Age               ",
            "keyColor": "blue",
            "text": "echo \"$(( ($(date +%s) - $(stat -c%W /)) / 86400 ))\" days"
        },
        {
            "type": "break",
            "key": "{icon}",
            "keyColor": "blue"
        },
        "colors"
    ]
