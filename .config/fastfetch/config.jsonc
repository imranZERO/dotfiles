            {
                "green": 55,
                "yellow": 70
            }
        },
        {
            "type": "display",
            "key": "{icon}  Monitor              ",
            "keyColor": "blue"
        },
        {
            "type": "memory",
            "key": "{icon}  Memory               ",
            "keyColor": "blue"
        },
        {
            "type": "swap",
            "key": "{icon}  Swap                 ",
            "keyColor": "blue"
        },
        {
            "type": "disk",
            "key": "{icon}  Hard Drive           ",
            "keyColor": "blue",
            "outputColor": "bright_cyan"
        },
        "break",
        {
            "type": "packages",
            "key": "{icon}  Installed Packages   ",
            "keyColor": "blue"
        },
        {
            "type": "shell",
            "key": "{icon}  Shell                ",
            "keyColor": "blue"
        },
        {
