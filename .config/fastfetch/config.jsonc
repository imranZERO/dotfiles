            "key": "{icon}  Terminal             ",
            "keyColor": "blue"
        },
        {
            "type": "localip",
            "key": "{icon}  IP Address           ",
            "keyColor": "blue"
        },
        {
            "type": "battery",
            "key": "{icon}  Battery              ",
            "keyColor": "blue"
        },
        {
            "type": "poweradapter",
            "key": "{icon}  Power Adapter        ",
            "keyColor": "blue"
