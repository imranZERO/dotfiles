{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo":
    {
        "padding":
        {
            "top": 7
        }
    },
    "general":
    {
        "multithreading": true
    },
    "display":
    {
        "separator": "âžœ    "
    },
    "modules":
    [
        {
            "type": "title",
            "key": "{icon}  User                 ",
            "keyColor": "blue"
        },
        "break",
        {
            "type": "host",
            "key": "{icon}  System               ",
            "keyColor": "blue",
            "outputColor": "bright_magenta"
        },
        {
            "type": "bios",
            "key": "{icon}  Firmware             ",
            "keyColor": "blue"
        },
        {
            "type": "os",
            "key": "{icon}  Operating System     ",
            "keyColor": "blue",
            "outputColor": "bright_cyan"
        },
        {
            "type": "kernel",
            "key": "{icon}  Kernel               ",
            "keyColor": "blue",
            "outputColor": "bright_yellow"
        },
        {
            "type": "cpu",
            "key": "{icon}  Processor            ",
            "keyColor": "blue",
            "outputColor": "bright_red",
            "temp":
            {
                "green": 55,
                "yellow": 70
            }
        },
        // {
        //     "type": "cpucache",
        //     "key": "{icon}  Cache                ",
        //     "keyColor": "blue"
        // },
        {
            "type": "gpu",
            "key": "{icon}  Graphic Card         ",
            "keyColor": "blue",
